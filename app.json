{
  "expo": {
    "name": "Tricky HR",
    "slug": "trickyhr",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "trickyhr",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "updates": {
      "enabled": false
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.stalinchidhamparam01.trickyhr"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#e46a23",
        "foregroundImage": "./assets/images/splash-icon.png",
        "backgroundImage": "./assets/images/splash-icon.png",
        "monochromeImage": "./assets/images/splash-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.stalinchidhamparam01.trickyhr"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#e46a23",
          "dark": {
            "backgroundColor": "#e46a23"
          }
        }
      ],
      "@react-native-community/datetimepicker"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5a4662ee-ee8d-4f50-9702-b7170cf65228"
      }
    }
  }
}
